<div class="homepage_cover">

  <div id= "myCarousel" class= "carousel slide">

    <div class="carousel-inner">

      <div class="item active">
        <%= image_tag("outdoor1.jpg", class: "img-responsive") %>
        <div class = "carousel-caption">
          <h2>Fitness App</h2>
          <h4>Find Instructors and classes near you.</h4>
        </div>
      </div>
        
        
        <div class="item">
        <%= image_tag("classadultyoga1.jpg", class: "img-responsive") %>
        <div class = "carousel-caption">
          <h2>Group Classes</h2>
          <h4>Book a private or group class, meet other health concious individuals.</h4>
        </div>
      </div>
        
        
        <div class="item">
        <%= image_tag("Instructor1.jpg", class: "img-responsive") %>
        <div class = "carousel-caption">
          <h2>Personal Instructor</h2>
          <h4>Work out with a fitness instructors near you.</h4>
        </div>
      </div>
    </div>
    <ol class= "carousel-indicators">
      <li data-target="#myCarousel"data-slide-to="0" class= "active"></li>
      <li data-target="#myCarousel"data-slide-to="1"></li>
      <li data-target="#myCarousel"data-slide-to="2"></li>
    </ol>
  </div>
  <a class= "carousel-control left" href = "#myCarousel" data-slide = "prev">
    <span class = "icon-prev"></span>

    <a class= "carousel-control right" href = "#myCarousel" data-slide = "next">
      <span class = "icon-next"></span>
  </a>
</div>